{
    "name": "mitophp/mito",
    "description": "A minimal Laravel package for blog publishing",
    "keywords": [
        "mitophp",
        "laravel",
        "mito"
    ],
    "homepage": "https://github.com/mitophp/mito",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Serv√≠n",
            "email": "oliver@radiocubito.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "livewire/livewire": "^2.6",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "nunomaduro/larastan": "^0.7.13",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-livewire": "^1.0",
        "spatie/laravel-ray": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Mito\\": "src",
            "Mito\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mito\\MitoServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
